<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>SNAP.CastleWindsor</id>
    <version>1.4</version>
    <authors>Tyler Brinks</authors>
    <description>
      Castle Windsor provider for SNAP. SNAP makes AOP easy by integrating with your favorite IoC container.  SNAP does not modify compiled code (unlike PostSharp) .
      It uses your IoC container to create runtime-callable wrappers to intercept your method calls.  Source code for SNAP is hosted at http://github.com/Acoustic/Snap
    </description>
    <language>en-US</language>
    <tags>SNAP, Simple Aspects, AOP, Aspect Oriented Programming, Castle</tags>
    <projectUrl>http://www.simpleaspects.com</projectUrl>
    <dependencies>
      <dependency id="SNAP" version="1.4" />
      <dependency id="Castle.Windsor" version="2.5.2" />
    </dependencies>
  </metadata>
</package>

